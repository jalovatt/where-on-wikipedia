{
  "name": "where-on-wikipedia",
  "version": "1.0.0",
  "description": "A Carmen Sandiego-style game powered by Wikipedia",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf dist",
    "copy": "shx mkdir dist && shx cp -r public views dist/",
    "compile": "npm run clean && npm run copy && babel \"./{,!(dist|node_modules)/**/}*.js\" bin/www -d dist",
    "start": "npm run compile && node ./dist/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalovatt/wiki-sandiego.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jalovatt/wiki-sandiego/issues"
  },
  "homepage": "https://github.com/jalovatt/wiki-sandiego#readme",
  "dependencies": {
    "bcrypt": "2.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "mongo": "^0.1.0",
    "mongodb": "^3.1.10",
    "morgan": "~1.9.0"
  }
}
